    "### 1. converting celcius to fahrenheit and vice versa"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "32.0\n",
      "7.222222222222222\n"
     ]
    }
   ],
   "source": [
    "def convert(temp,unit):\n",
    "    if(unit=='C'):\n",
    "        return temp*9/5+32\n",
    "    if(unit=='F'):\n",
    "        return (temp-32)*5/9\n",
    "    else:\n",
    "        return None\n",
    "print(convert(0,'C'))\n",
    "print(convert(45,'F'))"
   ]
  },
